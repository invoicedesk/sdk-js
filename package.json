{
  "name": "@invoicedesk/client",
  "version": "2.0.1",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/invoicedesk/sdk-js.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "codegen": "openapi --input ./api-spec.json --output ./src/codegen --useOptions --indent 2 --postfixServices API --client axios --name NeedfullAPI",
    "docs": "typedoc --excludePrivate --excludeProtected --hideParameterTypesInTitle --includeVersion --hideGenerator --hideParameterTypesInTitle --readme ./README.md --excludeReferences --plugin suika-docs-theme --theme suika-theme src/index.ts",
    "publish": "npm publish",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "openapi-typescript-codegen": "^0.25.0",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "suika-docs-theme": "^1.0.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-retry": "^3.6.0"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "@invoicedesk/sdk"
  ]
}
